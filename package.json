    "expo-status-bar": "~1.1.0",
    "expo-web-browser": "~10.0.3",
    "intl": "^1.2.5",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-native": "0.64.3",
    "react-native-elements": "^3.4.2",
    "react-native-gesture-handler": "~1.10.2",
    "react-native-reanimated": "~2.2.0",
