    "@react-navigation/native-stack": "^6.1.0",
    "expo": "~43.0.2",
    "expo-asset": "~8.4.3",
    "expo-cli": "^6.0.1",
    "expo-constants": "~12.1.3",
    "expo-dev-client": "~0.6.3",
    "expo-font": "~10.0.3",
    "expo-linking": "~2.4.2",
